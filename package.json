{
  "name": "@codexcentral/mask-text-to-symbol",
  "version": "1.1.0",
  "description": "Takes a string as input and returns a new string with all characters replaced by an specified symbol.",
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "typescript",
    "string",
    "replace",
    "text",
    "mask",
    "password",
    "javascript",
    "node"
  ],
  "author": "Roberto Silva Zu√±iga <c.roberto.u@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Codex-Central/mask-text-to-symbol.git"
  },
  "bugs": {
    "url": "https://github.com/Codex-Central/mask-text-to-symbol/issues"
  },
  "homepage": "https://github.com/Codex-Central/mask-text-to-symbol#readme",
  "scripts": {
    "build": "rm -rf dist/ && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc --outDir dist/esm --module esnext",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs"
  },
  "devEngines": {
    "node": ">=18.x",
    "npm": ">=9.x"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
